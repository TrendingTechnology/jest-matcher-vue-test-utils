{
  "name": "jest-matcher-vue-test-utils",
  "version": "1.6.0",
  "description": "Jest matchers to test Vue components easily",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch -o"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hmsk/jest-matcher-vue-test-utils.git"
  },
  "keywords": [
    "vue",
    "test",
    "jest",
    "matcher"
  ],
  "author": "Kengo Hamasaki <k.hamasaki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmsk/jest-matcher-vue-test-utils/issues"
  },
  "homepage": "https://github.com/hmsk/jest-matcher-vue-test-utils#readme",
  "devDependencies": {
    "@types/jest": "24.0.16",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "jest": "24.8.0",
    "rollup": "1.17.0",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript2": "0.22.1",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3",
    "vue": "2.6.10",
    "vue-jest": "3.0.4",
    "vue-template-compiler": "2.6.10",
    "vuex": "3.1.1"
  },
  "peerDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "jest": ">=22.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./test/tsconfig.json"
      },
      "vue-jest": {
        "babelConfig": false
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "testURL": "http://localhost/",
    "preset": "ts-jest"
  }
}
